import java.util.*;
class Maths{
    void sub(int a,int ab){
        System.out.println(a+ab);
    }void Mul(int b,int ab){
        System.out.println(b-ab);
    }void Div(int c,int ac){
        System.out.println(c*ac);
    }

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        Maths sum= new Maths();
        sum.sub(20,20);
        sum.Div(20,20);
        sum.Mul(20,20);
    }
}
