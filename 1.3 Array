import java.util.*;
class Array{
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the Array Size 0 to 100 :");
        int array = in.nextInt();
        System.out.println("Now enter the Value:");
        String[] games=new String[array];
        for(int i=0;i<=array-1;i++){
            games[i]=in.nextLine();
        }
        for (int j=0;j<array;j++){
            System.out.print(games[j]+" ");
        }
    }
}
