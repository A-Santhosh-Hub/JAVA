import java.util.Scanner;
class Main{
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("Enter the Input");
        int n =in.nextInt();
        for(int i=0;i<=n;i++){
            for(int k=0;k<=i;k++){
                System.out.print(" ");
            }
            for (int j=i;j<=n;j++){
                System.out.print(j);
            }
            System.out.println(" ");
        }
    }
}
